                  option sort="Ло" value="spec303"  Лор (отоларинголог)
                  option sort="Ок" value="spec310"  Окулист (офтальмолог)
                  option sort="Ре" value="spec320"  Ревматолог
                  option sort="Ре" value="spec379"  Рентгенолог
                  option sort="Те" value="spec324"  Терапевт
                  option sort="Те" value="spec389"  Терапевт участковый
                  option sort="Ур" value="spec327"  Уролог
                  option sort="Фи" value="spec405"  Физиотерапевт
                  option sort="Фу" value="spec395"  Фунциональный диагност
                  option sort="Хи" value="spec329"  Хирург
            .modal-body
              .section id="doctorList" class="doc-list" style="overflow: hidden;"
                hidden
