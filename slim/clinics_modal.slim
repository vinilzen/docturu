                    section.doctor_2_0.spec299.spec0.spec0.spec0.spec0 data-address=("Наличная ул.37") data-cliniclogo="https://cdn.doctu.ru/images/thumb/74x74/74x74-no-logo-big.jpg" data-clinicphone="" data-clinicurl="p4.doctu.ru/vzroslaya_53" data-doctor="vNaQHv5VqyDj2ZA7" data-family="Александров" data-metro=("<span class='line3'>Приморская</span> <span class='line3'>Василеостровская</span> <span class='line5'>Спортивная</span> ") data-name=("Павел Андреевич") data-photo="https://cdn.doctu.ru/images/thumb/74x110/74x110-22bd6ba4dd35098e0298acc9d2a62926.jpg" data-speciality="Инфекционист" 
